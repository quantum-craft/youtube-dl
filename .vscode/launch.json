{
  "configurations": [
    {
      "name": "Debug: youtube_dl/__main__.py",
      "type": "debugpy",
      "request": "launch",
      "program": "youtube_dl/__main__.py",
      "console": "integratedTerminal",
      "env": { "PYTHONPATH": "${workspaceFolder}" },
      "args": "--no-cache-dir -a dataset_test.txt -o 'test/%(id)s_%(duration)s_%(title)s.%(ext)s'"
    }
  ]
}

